	<script>
		$(document).ready(function () {
			$('.datetimepicker').datetimepicker({
				format: 'L',
				icons: {
					time: "fa fa-clock-o",
					date: "fa fa-calendar",
					up: "fa fa-chevron-up",
					down: "fa fa-chevron-down",
					previous: 'fa fa-chevron-left',
					next: 'fa fa-chevron-right',
					today: 'fa fa-screenshot',
					clear: 'fa fa-trash',
					close: 'fa fa-remove'
				}
			});

		});

		$("#btnCadastrar").on('click', function(){
			if($('#cardCadastrar').hasClass('d-none') || $('#footerCadastrar').hasClass('d-none') ){
				
				$('#cardCadastrar').removeClass('d-none')
				$('#footerCadastrar').removeClass('d-none')
				$('#cardLogin').addClass('d-none')
				$('#footerLogin').addClass('d-none')
				$('#cardTitle').text("Cadastrar")

			}else if($('#cardLogin').hasClass('d-none') || $('#footerLogin').hasClass('d-none')){ 
				$('#cardCadastrar').addClass('d-none')
				$('#footerCadastrar').addClass('d-none')
				$('#cardLogin').removeClass('d-none')
				$('#footerLogin').removeClass('d-none')

			}
		})

		$("#cadastrar").on('click', function(){
			
			var user = {
				primeiroNome: $("#cdt_firstName").val(),
				ultimoNome: $("#cdt_lastName").val(),
				email: $("#cdt_email").val(),
				senha: $("#cdt_senha").val(),
				status: 1,
				dataCadastro: new Date(),
				genero: $("#cdt_masculino").is(':checked') == true ? 1 : 0, 
				dataFormatura: new Date($("#cdt_formacao").val())
			}

			$.ajax({
				type : "POST",
				url: '/cadastro/newuser',
				data: user,
				success: function(results){
					console.log(results)
					$('#modalbutton').click()
				},
				error: function(error){
					console.log(error)
				}
			})

	





		})

		var _gaq = _gaq || [];

		_gaq.push(['_setAccount', 'UA-46172202-1']);
